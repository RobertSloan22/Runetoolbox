{
  "manifest_version": 3,
  "name": "Keystroke and Website Tracker",
  "version": "1.0",
  "description": "A Chrome extension to track and save keystrokes and visited websites.",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "history"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
